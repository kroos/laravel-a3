# wz that needed for reset rebirth.
A3_HERO_RESET_REBIRTH_WZ=2000000000


# mercenary rebirth
# at what level mercenary can start rebirth.
A3_MERCENARY_REBIRTH_LEVEL=150
# wz needed for rebirth.
