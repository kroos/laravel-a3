# at what level players can start to buy skill. default is 150
A3_LEVEL_BUY_SKILL=150

# equip super super shue
A3_WZ_EQUIP_SUPER_SUPER_SHUE=500000000

# buy lore
#how much wz you wanna sell for 1 lore, default is 1 lore = 100wz.
